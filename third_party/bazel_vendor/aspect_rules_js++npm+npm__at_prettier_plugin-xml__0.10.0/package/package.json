{
  "name": "@prettier/plugin-xml",
  "version": "0.10.0",
  "description": "prettier plugin for XML",
  "main": "src/plugin.js",
  "scripts": {
    "lint": "eslint --cache .",
    "print": "prettier --plugin=.",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prettier/plugin-xml.git"
  },
  "author": "Kevin Deisz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prettier/plugin-xml/issues"
  },
  "homepage": "https://github.com/prettier/plugin-xml#readme",
  "dependencies": {
    "@xml-tools/parser": "^1.0.2",
    "prettier": ">=1.10"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.10.1",
    "jest": "^26.0.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "env": {
      "es6": true,
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ]
    }
  },
  "jest": {
    "testRegex": ".test.js$"
  },
  "prettier": {
    "trailingComma": "none"
  }
}
